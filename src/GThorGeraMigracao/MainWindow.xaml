<views:GThorWindow x:Class="GThorGeraMigracao.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:views="clr-namespace:GThorFrameworkWpf.Views;assembly=GThorFrameworkWpf"
        mc:Ignorable="d"
        Style="{StaticResource ComercialWindow}"
        WindowState="Normal"
        LoadedCommand="{Binding LoadedCommand}"
        WindowStartupLocation="CenterScreen"
        Background="{StaticResource AccentColorBrush}"
        Title="GERA MIGRAÇÃO" Height="190" Width="525">
    <StackPanel Margin="24">
        <TextBlock Style="{StaticResource ControlForm}">Nome do Arquivo de migração, não pode conter espaços</TextBlock>
        <TextBox Text="{Binding NomeArquivoMigracao}"></TextBox>
        <Button Command="{Binding SalvarCommand}" Style="{StaticResource ComercialSuccessButton}" Margin="0,8,0,0" Height="45">Criar Migração</Button>
    </StackPanel>
</views:GThorWindow>
