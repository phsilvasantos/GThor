<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping assembly="GThorFrameworkDominio"
                   namespace="GThorFrameworkDominio.Dominios.EstadosUf"
                   xmlns="urn:nhibernate-mapping-2.2">

  <class name="Uf" table="uf">
    
    <id name="Id" column="id" generator="native"/>
    
    <property name="Sigla" column="sigla" length="2" not-null="true"/>
    
    <property name="Nome" column="nome" length="100" not-null="true"/>
    
    <property name="CodigoIbge" column="codigoIbge" not-null="true"/>
    
  </class>  
  
</hibernate-mapping>